{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "tsc -b && vite build && echo 'Bundle analysis available at dist/stats.html'",
    "lighthouse:dev": "lighthouse http://localhost:5173 --view",
    "lighthouse:prod": "lighthouse http://localhost:4173 --view",
    "lighthouse:ci": "lighthouse http://localhost:4173 --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless'",
    "perf": "npm run build:analyze && npm run preview && npm run lighthouse:ci",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@reduxjs/toolkit": "^2.8.2",
    "@stomp/stompjs": "^7.1.1",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.7.1",
    "sockjs-client": "^1.6.1",
    "tailwindcss": "^4.1.11",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sockjs-client": "^1.5.4",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
